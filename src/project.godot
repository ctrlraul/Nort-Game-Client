boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://Images/branding/splash_screen.png"
config/icon="res://Images/branding/icon256x256.png"
config/windows_native_icon="res://images/branding/icon16x16.ico"
boot_splash/minimum_display_time=500

[autoload]

DragManager="*res://Nort/DragManager.cs"
PagesNavigator="*res://Nort/PagesNavigator.cs"
Game="*res://Nort/Game.cs"
Console="*res://Scenes/Hud/Console.tscn"
Stage="*res://Scenes/Stage.tscn"
PopupsManager="*res://Nort/PopupsManager.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/energy_saving/keep_screen_on=false

[dotnet]

project/assembly_name="Nort"

[filesystem]

import/blender/enabled=false

